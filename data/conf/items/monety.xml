<?xml version="1.0" encoding="UTF-8"?>
<items xmlns="stendhal" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="stendhal ../items.xsd ">

	<item name="miedziaki">
		<type class="monety" subclass="miedziaki" tileid="-1"/>
		<description>Oto miedziaki. Podstawowy środek płatniczy najniższych klas społecznych.</description>
		<implementation class-name="games.stendhal.server.entity.item.StackableItem"/>
		<attributes>
			<quantity value="1"/>
		</attributes>
		<weight value="0.01"/>
		<value value="1"/>
		<equipable>
			<slot name="ground"/>
			<slot name="content"/>
			<slot name="money"/>
			<slot name="lhand"/>
			<slot name="rhand"/>
			<slot name="bag"/>
			<slot name="trade"/>
		</equipable>
	</item>

	<item name="talary">
		<type class="monety" subclass="talary" tileid="-1"/>
		<description>Oto srebrne talary. Podstawowy środek płatniczy średnich klas społecznych.</description>
		<implementation class-name="games.stendhal.server.entity.item.StackableItem"/>
		<attributes>
			<quantity value="1"/>
		</attributes>
		<weight value="0.01"/>
		<value value="100"/>
		<equipable>
			<slot name="ground"/>
			<slot name="content"/>
			<slot name="money"/>
			<slot name="lhand"/>
			<slot name="rhand"/>
			<slot name="bag"/>
			<slot name="trade"/>
		</equipable>
	</item>

	<item name="dukaty">
		<type class="monety" subclass="dukaty" tileid="-1"/>
		<description>Oto złote dukaty. Środek płatniczy najwyżych klas społecznych.</description>
		<implementation class-name="games.stendhal.server.entity.item.StackableItem"/>
		<attributes>
			<quantity value="1"/>
		</attributes>
		<weight value="0.01"/>
		<value value="10000"/>
		<equipable>
			<slot name="ground"/>
			<slot name="content"/>
			<slot name="money"/>
			<slot name="lhand"/>
			<slot name="rhand"/>
			<slot name="bag"/>
			<slot name="trade"/>
		</equipable>
	</item>
</items>